# AI-Enhanced Cashback Scraper Environment Configuration

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-3.5-turbo
OPENAI_MAX_TOKENS=500
OPENAI_TEMPERATURE=0.1

# Alternative AI Providers (uncomment to use)
# ANTHROPIC_API_KEY=your_anthropic_key_here
# HUGGINGFACE_API_KEY=your_hf_key_here

# AI Agent Configuration
AI_EXTRACTION_ENABLED=true
PATTERN_LEARNING_ENABLED=true
ADAPTIVE_SELECTOR_ENABLED=true

# Scraping Configuration
DEFAULT_DELAY=1
MAX_WORKERS=3
REQUEST_TIMEOUT=10
RETRY_ATTEMPTS=3

# Learning and Adaptation
SAVE_LEARNED_PATTERNS=true
MIN_CONFIDENCE_THRESHOLD=0.7
PATTERN_LEARNING_FILE=learned_patterns.json

# Logging
LOG_LEVEL=INFO
LOG_AI_DECISIONS=true
LOG_FILE_PREFIX=ai_scraper
